{
  "name": "yarnjs",
  "version": "0.5.0",
  "description": "spin your own yarn",
  "main": "index.js",
  "bin": {
    "yarn": "./bin/index.js"
  },
  "scripts": {
    "preversion": "babel lib --out-dir lib-es5 && cp lib/config/defaults.yml lib-es5/config/defaults.yml",
    "test": "mocha --recursive --compilers js:babel/register --reporter spec",
    "test:unit": "npm test -- test/unit",
    "test:e2e": "npm test -- test/e2e",
    "lint": "eslint ."
  },
  "repository": "yarnjs/yarn",
  "keywords": [
    "blog",
    "blogging",
    "static",
    "static site",
    "static site generator"
  ],
  "author": "Harry Wolff <hello@hswolff.com> (http://hswolff.com/)",
  "license": "MIT",
  "dependencies": {
    "babel": "^5.8.23",
    "babel-runtime": "^5.8.20",
    "babelify": "^6.3.0",
    "bluebird": "^2.9.34",
    "browserify": "^11.0.1",
    "chokidar": "^1.0.5",
    "fs-extra": "^0.24.0",
    "glob": "^5.0.14",
    "gray-matter": "^2.0.1",
    "highlight.js": "^8.7.0",
    "http-server": "^0.8.0",
    "inquirer": "^0.9.0",
    "js-yaml": "^3.4.0",
    "less": "^2.5.1",
    "less-plugin-autoprefix": "^1.4.2",
    "less-plugin-clean-css": "^1.5.1",
    "lodash": "^3.10.1",
    "moment": "^2.10.6",
    "nunjucks": "^2.0.0",
    "remarkable": "^1.6.0",
    "rimraf": "^2.4.3",
    "uglifyify": "^3.0.1",
    "winston": "^1.0.1",
    "yargs": "^3.23.0",
    "yarn-excerpt": "0.5.0",
    "yarn-html-minifier": "0.5.0",
    "yarn-scaffold": "^0.5.0"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.1",
    "chai": "^3.2.0",
    "eslint": "^1.3.1",
    "mocha": "^2.3.0",
    "mocha-osx-reporter": "^0.1.2",
    "sinon": "^1.16.1"
  }
