{
  "name": "yarnjs",
  "version": "0.8.2",
  "description": "spin your own yarn",
  "main": "./dist/lib/index.js",
  "scripts": {
    "compile": "babel lib --out-dir dist/lib && babel bin --out-dir dist/bin",
    "prepublish": "npm run compile && cp lib/config/defaults.yml dist/lib/config/defaults.yml",
    "test": "mocha --recursive --require ./test/setup.js --reporter spec",
    "test:unit": "npm test -- test/unit",
    "test:e2e": "npm test -- test/e2e",
    "lint": "eslint .",
    "coverage": "babel-node ./node_modules/.bin/isparta cover ./node_modules/mocha/bin/_mocha -- --recursive",
    "coveralls": "babel-node ./node_modules/.bin/isparta cover --report lcovonly ./node_modules/mocha/bin/_mocha -- --recursive && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "repository": "yarnjs/yarn",
  "keywords": [
    "blog",
    "blogging",
    "static",
    "static site",
    "static site generator"
  ],
  "author": "Harry Wolff <hello@hswolff.com> (http://hswolff.com/)",
  "license": "MIT",
  "engines": {
    "node": ">=4.0.0"
  },
  "dependencies": {
    "activity-logger": "^1.4.0",
    "babel-polyfill": "^6.5.0",
    "babel-runtime": "^6.5.0",
    "babelify": "^7.2.0",
    "bluebird": "^3.3.3",
    "browserify": "^13.0.0",
    "chalk": "^1.1.1",
    "chokidar": "^1.4.3",
    "find-up": "^1.1.0",
    "fs-extra": "^0.26.5",
    "glob": "^7.0.0",
    "gray-matter": "^2.0.2",
    "highlight.js": "^9.2.0",
    "http-server": "^0.9.0",
    "inquirer": "^0.12.0",
    "js-yaml": "^3.5.3",
    "less": "^2.6.0",
    "less-plugin-autoprefix": "^1.5.1",
    "less-plugin-clean-css": "^1.5.1",
    "less-plugin-npm-import": "yarnjs/less-plugin-npm-import#basedir",
    "lodash": "^4.5.1",
    "moment": "^2.11.2",
    "node-sass": "^3.4.2",
    "nunjucks": "^2.3.0",
    "remarkable": "^1.6.2",
    "resolve": "^1.1.7",
    "rimraf": "^2.5.2",
    "slug": "^0.9.1",
    "uglifyify": "^3.0.1",
    "yargs": "^4.2.0",
    "yarn-excerpt": "0.6.0",
    "yarn-html-minifier": "^0.6.1",
    "yarn-scaffold": "^0.6.3"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-eslint": "^5.0.0",
    "babel-plugin-espower": "^2.1.2",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.5.2",
    "coveralls": "^2.11.8",
    "eslint": "^2.2.0",
    "isparta": "^4.0.0",
    "mocha": "^2.4.5",
    "power-assert": "^1.2.0",
    "sinon": "^1.17.3"
  }
}
